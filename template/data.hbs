<!DOCTYPE html>
<html>
  <head>
    <title>Data</title>
    <link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.6.0/pure-min.css">
    <style>.page{margin: 50px auto;width:750px}</style>
  </head>
  <body>
    <div class="page">
      <h1>Data</h1>
      {{#if data.accelerometer}}
        <h2>Accelerometer</h2>
        {{table data.accelerometer}}
      {{/if}}
      {{#if data.gyroscope}}
        <h2>Gyroscope</h2>
        {{table data.gyroscope}}
      {{/if}}
      {{#if data.hrm}}
        <h2>Heart Rate</h2>
        {{table data.hrm}}
      {{/if}}
    </div>
  </body>
</html>
